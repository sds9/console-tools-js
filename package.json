  },
  "homepage": "https://github.com/sds9-org/js-exec#readme",
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@typescript-eslint/eslint-plugin": "^8.29.1",
    "@typescript-eslint/parser": "^8.29.1",
    "eslint": "^9.25.0",
    "globals": "^16.0.0",
    "jiti": "^2.4.2",
    "typescript": "^5.8.3",
